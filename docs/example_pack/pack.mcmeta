{
    "pack": {
        "pack_format": 46,
        "min_format": 46,
        "max_format": 69,
        "supported_formats": {
            "min_inclusive": 46,
            "max_inclusive": 69
        },
        "description": "Catharsis example pack"
    },
    "catharsis:pack/v1": {
        "id": "example_pack",
        "version": "1.0.0",
        "dependencies": {
            "catharsis": ">=1.0.0-beta.5"
        },
        "config": [
            {
                "type": "tab",
                "title": "Interesting Tab Title",
                "options": [
                    {
                        "type": "dropdown",
                        "id": "dropdown_example",
                        "title": "Example Dropdown Title",
                        "description": "An example dropdown description.",
                        "options": [
                            {
                                "value": "a",
                                "text": "Option A",
                                "default": true
                            },
                            {
                                "value": "b",
                                "text": "Option B"
                            },
                            {
                                "value": "c",
                                "text": "Option C"
                            }
                        ]
                    }
                ]
            },
            {
                "type": "separator",
                "title": "Separator Example",
                "description": "Can even have a description!"
            },
            {
                "type": "boolean",
                "id": "boolean_example",
                "title": "Example Boolean Title",
                "description": {
                    "text": "",
                    "extra": [
                        {
                            "text": "Can even have color, ",
                            "color": "red"
                        },
                        {
                            "text": "or not."
                        }
                    ]
                },
                "default": true
            }
        ]
    },
    "fabric:overlays": {
        "entries": [
            {
                "directory": "example_boolean_overlay",
                "condition": {
                    "condition": "catharsis:config",
                    "pack": "example_pack",
                    "id": "boolean_example"
                }
            },
            {
                "directory": "example_dropdown_a_overlay",
                "condition": {
                    "condition": "fabric:not",
                    "value": {
                        "condition": "catharsis:config",
                        "pack": "example_pack",
                        "id": "dropdown_example",
                        "value": "a"
                    }
                }
            },
            {
                "directory": "example_dropdown_b_overlay",
                "condition": {
                    "condition": "catharsis:config",
                    "pack": "example_pack",
                    "id": "dropdown_example",
                    "value": "b"
                }
            },
            {
                "directory": "example_dropdown_c_overlay",
                "condition": {
                    "condition": "catharsis:config",
                    "pack": "example_pack",
                    "id": "dropdown_example",
                    "value": "c"
                }
            }
        ]
    }
}
